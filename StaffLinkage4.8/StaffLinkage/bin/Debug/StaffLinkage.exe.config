    <add key="230" value="03"/>
    <!-- 250:手術部看護師 -->
    <add key="250" value="03"/>
    <!-- 04:事務その他 -->
    <!-- 150:医学生 -->
    <add key="150" value="04"/>
    <!-- 206:保育士 -->
    <add key="206" value="04"/>
    <!-- 208:介護福祉士 -->
    <add key="208" value="04"/>
    <!-- 300:臨床検査技師 -->
    <add key="300" value="04"/>
    <!-- 350:臨床工学士 -->
    <add key="350" value="04"/>
