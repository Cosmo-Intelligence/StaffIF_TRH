    <add key="390" value="04"/>
    <!-- 500:薬剤師 -->
    <add key="500" value="04"/>
    <!-- 510:薬剤師化学療法 -->
    <add key="510" value="04"/>
    <!-- 550:薬剤助手 -->
    <add key="550" value="04"/>
