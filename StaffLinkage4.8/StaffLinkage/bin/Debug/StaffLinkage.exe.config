    <add key="140" value="01"/>
    <!-- 02:技師 -->
    <!-- 400:放射線技師 -->
    <add key="400" value="02"/>
    <!-- 410:医学物理士 -->
    <add key="410" value="02"/>
    <!-- 03：看護師 -->
    <!-- 200:看護師 -->
    <add key="200" value="03"/>
